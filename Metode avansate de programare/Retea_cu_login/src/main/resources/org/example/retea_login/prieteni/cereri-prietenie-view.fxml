                <TableColumn prefWidth="198.66668701171875" text="Nume" fx:id="numePrieten"/>
            </columns>
        </TableView>
        <Button layoutX="488.0" layoutY="200.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="71.0" text="Accepta" onAction="#handleAcceptPrieten" />
      <Button layoutX="488.0" layoutY="252.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="71.0" text="Respinge" onAction="#handleRejectPrieten"/>
