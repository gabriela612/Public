            <font>
                <Font name="System Bold" size="20.0" />
            </font>
        </Button>
        <Button layoutX="165.0" layoutY="21.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="137.0" text="Sugestii" onAction="#initSugestii">
